{
    "name": "high-five/scaffold",
    "description": "High Five project scaffolding",
    "version": "0.0.1",
    "type": "project",
    "license": "GPL-3.0-or-later",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "High Five",
            "email": "hello@high-five.dev",
            "homepage": "https://www.high-five.dev"
        },
        {
            "name": "Harm Putman",
            "email": "harm@high-five.dev",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "support@high-five.dev"
    },
    "require": {
        "php": "~8.3.0",
        "craftcms/cms": "^5.4"
    },
    "scripts": {
        "post-root-package-install": [
            "make hello"
        ]
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "8.3"
        },
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}
